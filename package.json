{
  "name": "full-stack-coding-challenge",
  "version": "1.0.0",
  "scripts": {
    "start:server": "npm run build:server && node dist/server/src/server.js",
    "build:server": "npm run clean && npx tsc -p server/tsconfig.json",
    "start:client": "npx webpack serve --env development",
    "build:client": "npx webpack --env production",
    "clean": "rm -rf ./dist",
    "postinstall": "node-config-ts",
    "test:client": "jest --config=client/jest.config.ts",
    "test:server": "NODE_ENV=test jest --config=server/jest.config.ts",
    "test": "npm run test:client && npm run test:server",
    "lint": "npx eslint --ext .ts,.tsx .",
    "husky": "husky install",
    "migrations:test": "NODE_ENV=test node dist/server/src/migrations.js",
    "migrations": "node dist/server/src/migrations.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salbadr/full-stack-coding-challenge.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/salbadr/full-stack-coding-challenge/issues"
  },
  "homepage": "https://github.com/salbadr/full-stack-coding-challenge#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.9.1",
    "@types/randomstring": "^1.1.7",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^27.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "npm-run-all": "^4.1.5",
    "randomstring": "^1.2.1",
    "string-replace-loader": "^3.0.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.4",
    "express": "^4.17.1",
    "fecha": "^4.2.1",
    "mysql": "^2.18.1",
    "node-config-ts": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  }
}
